#include <iostream>

using namespace std;

int main()
{
    int S = 0;  // сумма чисел
    int n = 1;  // номер числа
    while (n<=1000) {   // цикл с while
        if (n % 7 == 0){    // условие перебора 
            S = S + n;      
        }   
        n = n + 4;  // шаг, равный 4
    }

    cout << S;
    return 0;
}
